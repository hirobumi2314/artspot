<h1>Listing artbases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artbases.each do |artbasis| %>
      <tr>
        <td><%= link_to 'Show', artbasis %></td>
        <td><%= link_to 'Edit', edit_artbasis_path(artbasis) %></td>
        <td><%= link_to 'Destroy', artbasis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artbase', new_artbasis_path %>
